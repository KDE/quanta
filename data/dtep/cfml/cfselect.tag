<!DOCTYPE TAGS>
<TAGS>
<tag name="cfselect">
	<attr name="name" />
	<attr name="size" />
	<attr name="required" type="list">
		<items>
			<item>Yes</item>
			<item>No</item>
		</items>
	</attr>
	<attr name="message" />
	<attr name="onerror" />
	<attr name="multiple" type="list">
		<items>
			<item>Yes</item>
			<item>No</item>
		</items>
	</attr>
	<attr name="query" />
	<attr name="selected" />
	<attr name="value" />
	<attr name="display" />
	<attr name="passthrough" />
</tag>
</TAGS>