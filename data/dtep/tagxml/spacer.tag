<!DOCTYPE TAGS>
<TAGS>
<tag name="spacer">

    <label>
        <text>Orientation</text>
        <location col="0" row="0" />
    </label>

    <attr name="orientation" type="list">
        <tooltip>defaults to horizontal </tooltip>
	    <items>
	        <item>h</item>
		<item>v</item>
	    </items>
        <location col="1" row="0" />
    </attr>


    <spacer orientation="v">
        <location col="0" row="9" colspan="2" />
    </spacer>

</tag>
</TAGS>
