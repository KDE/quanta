<!DOCTYPE TAGS>
<TAGS>
<tag name="whiteSpace">
  <label>
    <text>value</text>
    <location col="0" row="0"/>
  </label>
  <attr name="value" type="list">
    <items>
      <item>preserve</item>
      <item>replace</item>
      <item>collapse</item>
    </items>
    <location col="1" row="0"/>
  </attr>

</tag>
</TAGS>
