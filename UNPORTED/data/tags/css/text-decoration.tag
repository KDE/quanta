<!DOCTYPE tags>
<tags>
  <tag name="text-decoration" type="property">
     <attr name="value" type="list">
       <items>
         <item>none</item>
         <item>underline</item>
		 <item>overline</item>
		 <item>line-through</item>
		 <item>blink</item>
         <item>inherit</item>
       </items>
     </attr>
  </tag>
</tags>
