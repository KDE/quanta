<!DOCTYPE TAGS>
<TAGS>
<tag name="cftransaction">
	<attr name="action" type="list">
		<items>
			<item>begin</item>
			<item>commit</item>
			<item>rollback</item>
		</items>
	</attr>
	<attr name="isolation" type="list">
		<items>
			<item>read_uncommitted</item>
			<item>read_committed</item>
			<item>repeatable_read</item>
			<item>serializable</item>
		</items>
	</attr>
</tag>
</TAGS>